{
    "name": "hi-folks/ghygen",
    "version": "0.6.2",
    "type": "project",
    "description": "Ghygen is a GitHub Actions configurator for your Laravel Application.",
    "keywords": [
        "github",
        "github-actions",
        "yaml",
        "cicd",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.0.2|^8.1|^8.2",
        "ext-json": "*",
        "composer/semver": "^3.2",
        "danharrin/livewire-rate-limiting": "^1.0",
        "doctrine/dbal": "^3.0",
        "fruitcake/laravel-cors": "^2.0",
        "guzzlehttp/guzzle": "^7.2",
        "hi-folks/lara-lens": "^0.4.0",
        "laravel/framework": "^9.19",
        "laravel/tinker": "^2.7",
        "livewire/livewire": "^2.6",
        "psalm/plugin-laravel": "^2.0",
        "swaggest/json-schema": "^0.12.31",
        "symfony/yaml": "^6.0"
    },
    "require-dev": {
        "brianium/paratest": "^6.2",
        "fakerphp/faker": "^1.9.1",
        "laravel/dusk": "^6.11",
        "laravel/pint": "^v1.4.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "nunomaduro/larastan": "^2.2.0",
        "pestphp/pest": "^1.22",
        "pestphp/pest-plugin-laravel": "^1.4",
        "pestphp/pest-plugin-livewire": "^1.0",
        "phpstan/phpstan": "^1.8.7",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^1.0",
        "vimeo/psalm": "^4.7|^5.4"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "all": [
            "@test", "@cs", "@phpstan"
        ],
        "test": "pest --order-by random",
        "cs":"pint",
        "phpstan": "phpstan analyse -c ./phpstan.neon --no-progress"
    }
}
